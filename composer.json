{
    "name": "zonuexe/php-checker",
    "description": "Simple PHP checker",
    "license": "MIT",
    "authors": [
        {
            "name": "USAMI Kenta",
            "email": "tadsan@zonu.me"
        }
    ],
    "require": {
        "nikic/php-parser": "^1.4.1"
    },
    "require-dev": {
        "theseer/phpdox":   "0.6.*"
        ,"phpmd/phpmd":     "@stable"
        ,"phploc/phploc":   "*"
        ,"phpunit/phpunit": "^4.8.14"
    },
    "autoload": {
        "psr-4": {
            "Teto\\PHPChecker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Teto\\PHPChecker\\": "tests/"
        }
    },
    "bin": [
        "bin/phpchecker"
    ]
}
